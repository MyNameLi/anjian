    -->
  <system.serviceModel>
    <diagnostics performanceCounters="All"/>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"/>
    <behaviors>
      <endpointBehaviors>
        <behavior name="webHttpEnablingBehaviour">
